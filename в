
int main()
  { float var1, var2;
   printf("(var1=3):");
   scanf("%f",&var1);
   printf("(var2=5)");
   scanf("%f",&var2);
   printf("var1>var2 %d\n", var1>var2);
   printf("var1<var2 %d\n", var1<var2);
   printf("var1==var2 %d\n", var1==var2);
   printf("var1>=var2 %d\n", var1>=var2);
   printf("var1<=var2 %d\n", var1<=var2);
   printf("var1!=var2 %d\n", var1!=var2);
   printf("!var1 %d\n, !var1");
   printf("!var2 %d\n, !var2");
   
    

    return 0;
}
